{
    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es6",
        "preserveConstEnums": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "module": "CommonJS",
        "lib": ["ES6"],
        "outDir": "dist",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "Node",
        "esModuleInterop": true      
    }
}